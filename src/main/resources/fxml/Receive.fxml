<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="com.soft1841.controller.ReceiveController"
            prefHeight="400.0" prefWidth="600.0">
    <ImageView fitHeight="700">
        <Image url="/img/topbj.jpg"/>
    </ImageView>
    <FlowPane fx:id="receivePane" hgap="10" vgap="10"
              AnchorPane.topAnchor="80"
              AnchorPane.rightAnchor="10"
              AnchorPane.leftAnchor="10"/>
    <Button text="新增收银员" AnchorPane.topAnchor="20"

            AnchorPane.leftAnchor="10" onAction="#addReceive" styleClass="button-blue"/>
</AnchorPane>
